package com.testspace.test_smart_sec_school_proposer;

import java.lang.Number;

rule "LoadSchoolData3"
	dialect "mvel"
	ruleflow-group "loadSchoolDataRuleFlowGroup"
	salience 80
	when
		$sch : School( )
		$notSelSch : NotSelectedSchool( $nSubSch : school, school.schoolName != $sch.schoolName )
	then
		SelectedSchool $selSchool = new SelectedSchool();
		$selSchool.setSchool( $sch );
		insert( $selSchool );
end
