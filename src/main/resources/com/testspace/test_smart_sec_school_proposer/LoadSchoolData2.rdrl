package com.testspace.test_smart_sec_school_proposer;

import java.lang.Number;

rule "LoadSchoolData2"
	dialect "mvel"
	ruleflow-group "loadSchoolDataRuleFlowGroup"
	salience 90
	when
		$student : Student( )
	then
		School $sch = new School();
		$sch.setSchoolName("Dummy Record");
		System.out.println("Dummy Record Insert to be executed" );
		NotSelectedSchool $NotSch = new NotSelectedSchool();
		$NotSch.setSchool( $sch );
		insert( $NotSch );
end
