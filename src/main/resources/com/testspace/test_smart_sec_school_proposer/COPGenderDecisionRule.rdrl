package com.testspace.test_smart_sec_school_proposer;

import java.lang.Number;

rule "COPGenderDecisionRule"
	ruleflow-group "copGenderDecisionRuleFlowGroup"
	dialect "mvel"
	salience 100
	when
		$stu : Student( )
		$selSch : SelectedSchool( $sch : school, school.copOverallMin > $stu.expectedScore )
	then
		// System.out.println("Executed Rule: " + drools.getRule().getName() );
		System.out.println("Executed Rule: " + drools.getRule().getName()  +  " Overall Min score - insert Not Selected Object : " + $sch.getSchoolName())
		NotSelectedSchool $NotSel = new NotSelectedSchool();
		$NotSel.setSchool( $sch );
		insert( $NotSel );
		retract( $selSch );
end
