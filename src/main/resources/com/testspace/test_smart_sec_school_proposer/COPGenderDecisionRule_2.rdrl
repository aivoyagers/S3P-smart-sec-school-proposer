package com.testspace.test_smart_sec_school_proposer;

import java.lang.Number;

rule "COPGenderDecisionRule_2"
	ruleflow-group "copGenderDecisionRuleFlowGroup"
	dialect "mvel"
	salience 90
	when
		$selSch : SelectedSchool( $sch : school)
		$student : Student( eval( !isStudentGenderAllowed($sch.getType()) ))
	then
		NotSelectedSchool $notSel = new NotSelectedSchool();
		$notSel.setSchool( $sch );
		insert( $notSel );
		retract( $selSch );
		System.out.println("Executed Rule: " + drools.getRule().getName() + " Gender Rule When condition met : " + $sch.getSchoolName() + " " + $sch.getType() );
end
